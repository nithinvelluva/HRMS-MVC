@if (@Model.Usertype == 1)
{
    Layout = "~/Views/Shared/_AdminDashboard.cshtml";
}
else
{
    Layout = "~/Views/Shared/_UserDetails.cshtml";
}
<html>
<head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <meta content='width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0' name='viewport' />
    <meta name="viewport" content="width=device-width" />

    <!-- Styles -->
    <link href="~/Content/fullcalendar.min.css" rel="stylesheet" />    
    <link href="~/Content/jquery.timesetter.css" rel="stylesheet" />
    <link href="~/Content/Dashboard/calendarView.css" rel="stylesheet" />  
    <link href="~/Content/ssi-uploader.css" rel="stylesheet" />    
    <link href="~/Content/jquery-selective.min.css" rel="stylesheet" />    
    <link href="~/Content/Dashboard/status style.css" rel="stylesheet" />    
    <link href="~/Content/jstree.min.css" rel="stylesheet" />
</head>
<body>
    <div class="calendar-view-default-modal" id="week-calendar-viewDiv">
        <div class="text-center">
            <label class="week-view-label"></label>
        </div>
        <div class="row">
            <div class="container-fluid">
                <div class="content">
                    <!-- Calendar view header starts -->
                    @Html.Partial("../Calendar/calendarViewHeader")
                    <!-- Calendar view header ends -->
                </div>
                <div id="week-calendar-view-event-load" class="load_div">
                    <i class="fa fa-refresh fa-spin fa-3x fa-fw"></i>
                    <span class="sr-only">Loading...</span>
                </div>
                <div class="row row-sep container-fluid" id="week-calendar-view">
                </div>
            </div>
        </div>        
    </div>
    <!-- Event popup view starts -->
    @Html.Partial("../Calendar/calendarTaskPopup")
    <!-- Event popup view ends -->
</body>
</html>
<!-- Scripts -->
<script src="~/Scripts/ssi-uploader.js"></script>
<script src="~/Scripts/app/calendar/calendarView.js"></script>